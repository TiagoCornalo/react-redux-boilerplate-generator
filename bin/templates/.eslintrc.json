{
  "env": {
      "browser": true,
      "es6": true,
      "es2021": true
  },
  "extends": "standard-with-typescript",
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "rules": {
      "react-refresh/only-export-components": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/no-explicit-any": "off"
    }
}